{
  "name": "obsidian-eagle-plugin",
  "version": "2.0.2",
  "type": "module",
  "description": "Upload images to Eagle and integrate with Obsidian notes.",
  "packageManager": "pnpm@10.28.2",
  "main": "main.js",
  "scripts": {
    "dev": "node scripts/dev.mjs",
    "dev:build": "node scripts/esbuild.build.js",
    "build": "node scripts/esbuild.build.js production",
    "ci": "pnpm run build && pnpm run lint && pnpm run test",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "e2e": "wdio run test/e2e/wdio.conf.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "eagle",
    "image-upload"
  ],
  "author": "gavvvr",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^8.57.1",
    "@inquirer/prompts": "^8.1.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "@vitest/ui": "^0.34.6",
    "@wdio/cli": "^8.26.2",
    "@wdio/local-runner": "^8.26.2",
    "@wdio/mocha-framework": "^8.26.2",
    "@wdio/spec-reporter": "^8.26.2",
    "builtin-modules": "3.3.0",
    "chromedriver": "^119.0.1",
    "esbuild": "^0.25.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-perfectionist": "^4.15.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-wdio": "^9.23.0",
    "globals": "^15.15.0",
    "obsidian": "^1.11.0",
    "obsidian-utils": "^0.10.2",
    "prettier": "^3.0.2",
    "tslib": "2.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vitest": "^0.34.6",
    "wdio-chromedriver-service": "^8.1.1"
  },
  "pnpm": {
    "overrides": {
      "axios": "^1.12.0",
      "diff": "^5.2.2",
      "esbuild": "^0.25.0",
      "fast-xml-parser": "^5.3.4",
      "form-data": "^4.0.4",
      "glob": "^10.5.0",
      "js-yaml": "^4.1.1",
      "lodash": "^4.17.23",
      "tar-fs": "^3.1.1",
      "tmp": "^0.2.4",
      "vite": "^5.4.21",
      "ws": "^8.17.1"
    }
  }
}
